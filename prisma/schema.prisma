generator client {
  provider = "prisma-client-js"
  moduleFormat = "cjs"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
  // ver despues para que ande localmente 
  // output        = "../node_modules/@prisma/client"
}

datasource db {
  provider = "postgresql"
}


model File {
  id        String      @id @default(uuid())
  fileName  String
  s3Url     String
  size      Int
  mimeType  String
  createdAt DateTime @default(now())
}